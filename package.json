    "firebase-functions": "^3.18.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "sequelize": "^6.26.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "eslint": "^8.29.0",
    "eslint-auto-fix": "^1.0.4",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^17.0.0",
